<Application x:Class="ArrivalAlarm.App" xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" d1p1:Ignorable="d"
             xmlns:d1p1="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:vm="using:LocationAlarm.ViewModel"
             xmlns:viewModel="using:LocationAlarm.ViewModel">
    <Application.Resources>
        <ResourceDictionary>
            <Style x:Name="CommandBarStyle" TargetType="CommandBar">
                <Setter Property="ClosedDisplayMode" Value="Compact" />
            </Style>
        </ResourceDictionary>
    </Application.Resources>
</Application>